{
  "name": "ai-inspector",
  "version": "1.0.0",
  "description": "Browser extension to inspect AI outputs for truth, alignment, and governance quality using mathematical assessment",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "serve": "node scripts/serve-dev.js",
    "dev-server": "npm run build && npm run serve",
    "type-check": "tsc --noEmit",
    "release": "npm run build && npm run package",
    "package": "node scripts/create-release.js"
  },
  "dependencies": {
    "dexie": "^3.2.4",
    "jszip": "^3.10.1",
    "mathjs": "^12.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "tailwind-scrollbar": "^3.1.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.258",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.9.0",
    "postcss": "^8.4.33",
    "postcss-loader": "^8.0.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
