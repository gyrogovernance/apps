  { value: 'epistemic', icon: 'üîç', label: 'Epistemic', description: 'Knowledge, communication, evidence' },
  { value: 'custom', icon: '‚úèÔ∏è', label: 'Custom', description: 'Your own category' }
];

const PLATFORMS: { value: Platform; label: string }[] = [
